# --- Required Variables ---

# Your public IP address for SSH access. Get it from: https://ifconfig.me/ip
# Example: ssh_cidr_blocks = ["1.2.3.4/32"]
ssh_cidr_blocks = ["REPLACE_WITH_YOUR_IP/32"]

# The name of your existing AWS EC2 Key Pair
key_name        = "your-aws-ssh-key-name"

